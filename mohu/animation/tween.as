package mohu.animation {	public function tween(target:Object, properties:Object, frames:int, easing:Function = null, onComplete:Function = null, onEnterFrame:Function = null, roundValues:Boolean = false):Animation {		var animation:Animation = new Animation(target, properties, frames, easing, roundValues);		if (onComplete != null) animation.onComplete.addListener(onComplete);		if (onEnterFrame != null) animation.onEnterFrame.addListener(onEnterFrame);		Animator.add(animation);		return animation;	}}