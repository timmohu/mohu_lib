package mohu.animation {	import flash.media.SoundTransform;		public function tweenSound(target:Object, soundTransform:SoundTransform, frames:int, easing:Function = null, onComplete:Function = null, onEnterFrame:Function = null):SoundEffect {		var soundEffect:SoundEffect = new SoundEffect(target, soundTransform, frames, easing);		if (onComplete != null) soundEffect.onComplete.addListener(onComplete);		if (onEnterFrame != null) soundEffect.onEnterFrame.addListener(onEnterFrame);		Animator.add(soundEffect);		return soundEffect;	}}