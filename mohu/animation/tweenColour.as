package mohu.animation {	import flash.display.DisplayObject;	import flash.geom.ColorTransform;				public function tweenColour(target:DisplayObject, colourTransform:ColorTransform, frames:int, easing:Function = null, onComplete:Function = null, onEnterFrame:Function = null):ColourEffect {		var colourEffect:ColourEffect = new ColourEffect(target, colourTransform, frames, easing);		if (onComplete != null) colourEffect.onComplete.addListener(onComplete);		if (onEnterFrame != null) colourEffect.onEnterFrame.addListener(onEnterFrame);		Animator.add(colourEffect);		return colourEffect;	}}