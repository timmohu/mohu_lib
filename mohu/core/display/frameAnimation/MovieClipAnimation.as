package mohu.core.display.frameAnimation {	import flash.display.MovieClip;	/**	 * @author Tim Kendrick	 * 	 * improves performance of animations by iterating thorugh the frames and caching them before playback	 */	public class MovieClipAnimation extends MovieClip {		public function MovieClipAnimation(_paused:Boolean = false) {			super();			while (currentFrame != totalFrames) nextFrame();			if (_paused) gotoAndStop(0); else gotoAndPlay(0);		}	}}